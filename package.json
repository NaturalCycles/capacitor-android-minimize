{
  "name": "capacitor-android-minimize",
  "version": "0.0.1",
  "description": "Ionic Capacitor plugin for minimizing Android apps",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Natural Cycles",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "typescript": "^3.2.4",
    "@capacitor/android": "latest"
  },
  "files": [
    "android/",
    "AndroidMinimize.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/capacitor-android-minimize"
  },
  "bugs": {
    "url": "https://github.com/NaturalCycles/capacitor-android-minimize/issues"
